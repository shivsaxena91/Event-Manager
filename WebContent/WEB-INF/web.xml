<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>event_manager</display-name>
  <welcome-file-list>
    <welcome-file>HomePageServlet</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HomePageServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.HomePageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomePageServlet</servlet-name>
    <url-pattern>/homepage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EventManagerServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.EventManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventManagerServlet</servlet-name>
    <url-pattern>/eventManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EventParticipantServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.EventParticipantServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventParticipantServlet</servlet-name>
    <url-pattern>/eventParticipant</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PurchaseTicketsServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.PurchaseTicketsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PurchaseTicketsServlet</servlet-name>
    <url-pattern>/purchaseTickets</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PasswordResetServlet</servlet-name>
    <servlet-class>com.eventmanager.servlets.PasswordResetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PasswordResetServlet</servlet-name>
    <url-pattern>/resetPassword</url-pattern>
  </servlet-mapping>
</web-app>